  "dependencies": {
    "@avelin/auth": "workspace:*",
    "@avelin/database": "workspace:*",
    "@avelin/id": "workspace:*",
    "@avelin/zero": "workspace:*",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/eden": "^1.2.0",
    "@elysiajs/swagger": "^1.2.0",
    "@mobily/ts-belt": "^3.13.1",
    "@t3-oss/env-core": "^0.11.1",
    "arctic": "catalog:",
    "elysia": "1.2.10",
    "jose": "^5.9.6",
    "remeda": "^2.19.2",
    "superjson": "^2.2.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@avelin/typescript-config": "workspace:*",
    "@types/bun": "^1.1.14",
    "typescript": "catalog:",
    "undici-types": "^7.2.3"
  }
